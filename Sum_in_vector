#include <iostream>
#include <vector>

int main() {
  int sum_even=1;
  int sum_odd=1;
  std::vector<int> num = {2, 4, 3, 6, 1, 9, 8, 0, 8, 5, 7, 3, 3};

  for(int i=0; i<num.size();i++){
    if(num[i] % 2 == 0){
      sum_even = sum_even+num[i];
    
    }
    else if(num[i]%2 !=0 ){
      sum_odd = sum_odd*num[i];
    }

  }
  std::cout << "The sum is "<< sum_even << "for even number\n";
    std::cout << "The sum is "<< sum_odd << "for odd number";
}
